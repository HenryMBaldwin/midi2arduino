{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build",
          "buildCommand": "npm run build"
        }
      },
      {
        "src": "api/midi2arduino.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/midi2arduino.py"
      },
      {
        "src": "/(.*)",
        "dest": "/build/$1"
      }
    ]
  }
  