{
    "version": 2,
    "builds": [
      {
        "src": "src/routes/**/*",
        "use": "@sveltejs/adapter-vercel"
      },
      {
        "src": "api/midi2arduino.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/midi2arduino.py"
      },
      {
        "src": "/(.*)",
        "dest": "/src/routes/$1"
      }
    ]
  }
  